<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="402.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.Controller.LogInController">
   <children>
      <AnchorPane layoutX="224.0" prefHeight="402.0" prefWidth="306.0" style="-fx-background-color: #1575F8;">
         <children>
            <TextField fx:id="usernameField" layoutX="106.0" layoutY="110.0" prefHeight="25.0" prefWidth="149.0" promptText="Username" />
            <Label fx:id="username" layoutX="33.0" layoutY="115.0" text="Username">
               <font>
                  <Font name="Arial Black" size="11.0" />
               </font></Label>
            <PasswordField fx:id="passwordLoginField" layoutX="106.0" layoutY="155.0" promptText="Password" />
            <Label fx:id="password" layoutX="35.0" layoutY="159.0" text="Password">
               <font>
                  <Font name="Arial Black" size="11.0" />
               </font></Label>
            <Button layoutX="106.0" layoutY="228.0" mnemonicParsing="false" onAction="#onLoginButtonClick" prefHeight="34.0" prefWidth="76.0" text="Login">
               <opaqueInsets>
                  <Insets top="5.0" />
               </opaqueInsets></Button>
          <Text fx:id="actionTarget" />
            <Button layoutX="181.0" layoutY="323.0" mnemonicParsing="false" onAction="#SignUpbutton" text="SignUp" />
            
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
